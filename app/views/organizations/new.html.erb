<h2>New Organization</h2>

<%= form_for(@organization, :html => {:class => "well form-horizontal"}) do |f| %>
  <%= render 'shared/error_messages_organization', :object => f.object %>
  <div class="control-group">
    <%= f.label :name, :class => "control-label" %>
    <div class="controls"><%= f.text_field :name %></div>
  </div>
  <div class="control-group">
    <%= f.label :founded, :class => "control-label" %>
    <div class="controls"><%= f.text_field :founded %></div>
  </div>
  <div class="form-actions">
    <%= f.submit "Create", :class => "btn" %>
    <%= link_to "Cancel", organizations_path, :class => "btn" %>
  </div>
<% end %>

